#include "../common.hlsl"

Texture2D<float4> t4 : register(t4);
Texture3D<float4> t3 : register(t3);
Texture2D<float4> t2 : register(t2);
Texture2D<float4> t1 : register(t1);
Texture2D<float4> t0 : register(t0);
SamplerState s4_s : register(s4);
SamplerState s3_s : register(s3);
SamplerState s2_s : register(s2);
SamplerState s1_s : register(s1);
SamplerState s0_s : register(s0);
cbuffer cb0 : register(b0){
  float4 cb0[11];
}

#define cmp -

void main(
  float4 v0 : SV_POSITION0,
  float4 v1 : TEXCOORD0,
  float2 v2 : TEXCOORD1,
  out float4 o0 : SV_Target0)
{
  float4 r0,r1,r2,r3,r4;
  uint4 bitmask, uiDest;
  float4 fDest;

  r0.xyzw = t0.Sample(s0_s, v2.xy).xyzw;
  r1.xyzw = t1.Sample(s2_s, v1.xy).xyzw;
  r1.xyzw = -r1.xyzw + r0.xyzw;
  r2.xyzw = sign(r1.xyzw);
  if(injectedData.toneMapType == 0.f){
  r1.xyzw = min(float4(1,1,1,1), abs(r1.xyzw));
  } else {
    r1.xyzw = abs(r1.xyzw);
  }
  r1.xyzw = r1.xyzw * r2.xyzw;
  r1.xyzw = r1.xyzw * cb0[4].yyyy + r0.xyzw;
  r2.x = cb0[4].x * 3 + 1;
  r2.x = 0.25 * r2.x;
  if(injectedData.toneMapType == 0.f){
    r1 = max(0.f, r1);
  r1.xyzw = log2(r1.xyzw);
  r1.xyzw = r2.xxxx * r1.xyzw;
  r1.xyzw = exp2(r1.xyzw);
  } else {
    r1.xyzw = renodx::math::SignPow(r1.xyzw, r2.x);
  }
  r2.xyz = cb0[3].zwy * float3(3,3,3) + float3(1,1,1);
  r2.xy = float2(0.25,0.25) * r2.xy;
  r3.xyz = r1.xyz * r2.xxx + float3(-0.5,-0.5,-0.5);
  r2.xyw = r2.yyy * r3.xyz + float3(0.5,0.5,0.5);
  r3.x = 0.0174532924 * cb0[2].w;
  r3.y = dot(float3(0.57735002,0.57735002,0.57735002), r2.xyw);
  r3.y = 0.57735002 * r3.y;
  sincos(r3.x, r3.x, r4.x);
  r3.z = 1 + -r4.x;
  r4.yzw = float3(0.57735002,0.57735002,0.57735002) * r2.wxy;
  r4.yzw = r2.wxy * float3(0.57735002,0.57735002,0.57735002) + -r4.wyz;
  r4.yzw = r4.yzw * r3.xxx;
  r3.xyz = r3.yyy * r3.zzz + r4.yzw;
  r2.xyw = r2.xyw * r4.xxx + r3.xyz;
  r3.x = dot(r2.xyw, float3(0.219999999,0.707000017,0.0710000023));
  r2.xyw = -r3.xxx + r2.xyw;
  r1.xyz = cb0[3].xxx * r2.xyw + r3.xxx;
  r2.y = r2.z * 0.125 + 0.5;
  r3.xyzw = r2.z >= 4 ? float4(-1,-1,1,1) : float4(-0,-1,0,1);
  r1.xyzw = r3.zzzw + r1.xyzw;
  r1.xyzw = r2.yyyy * r1.xyzw + r3.xxxy;
  r2.x = cb0[6].x * 3 + 1;
  r2.y = 0.25 * r2.x;
  r3.zw = r1.zx / r2.yy;
  r3.xy = r2.yy * r1.xz;
  r2.xy = (r2.x > 0) ? r3.xz : r3.wy;
  r2.xy = r2.xy < cb0[6].yy ? float2(0,0) : r2.xy;
  r2.z = 1 + -cb0[6].y;
  r3.xy = cmp(r2.zz < r2.xy);
  r2.w = r1.y < cb0[6].y ? 0 : r1.y;
  r2.z = r2.z < r2.w ? 1 : r2.w;
  r2.xy = r3.xy ? float2(1,1) : r2.xy;
  r3.x = cb0[2].y * r2.z;
  r2.xy = cb0[2].xz * r2.xy;
  r2.zw = float2(0.125,0.625);
  r4.xyzw = t2.Sample(s1_s, r2.xz).xyzw;      // strip LUT
  r3.y = 0.375;
  r3.xyzw = t2.Sample(s1_s, r3.xy).xyzw;
  r3.xyz = float3(0,1,0) * r3.xyz;
  r2.xyzw = t2.Sample(s1_s, r2.yw).xyzw;
  r3.xyz = r4.xyz * float3(1,0,0) + r3.xyz;
  r2.xyz = r2.xyz * float3(0,0,1) + r3.xyz;
  r3.xyz = r2.xyz * float3(255,255,255) + -cb0[6].www;
  if(injectedData.toneMapType == 0.f){
  r3.xyz = max(float3(0,0,0), r3.xyz);
  }
  r3.w = cb0[7].x + -cb0[6].w;
  r3.xyz = r3.xyz / r3.www;
  if(injectedData.toneMapType == 0.f){
    r3.xyz = saturate(r3.xyz);
  }
  r3.w = cb0[7].y + -cb0[7].z;
  r3.xyz = r3.xyz * r3.www + cb0[7].zzz;
  r3.xyz = float3(0.00392156886,0.00392156886,0.00392156886) * r3.xyz;
  r1.xyz = asint(cb0[6].z) == 1 ? r3.xyz : r2.xyz;
  r2.y = dot(r1.xyz, float3(0.219999999,0.707000017,0.0710000023));
  r2.y = 1.25 * r2.y;
  r3.xyzw = cb0[8].xyzw * float4(0.5,0.5,0.5,0.5) + r2.yyyy;
  r3.xyzw = float4(-0.333000004,-0.333000004,-0.333000004,-1) + r3.xyzw;
  r2.y = 0.5 * cb0[9].x;
  r3.xyzw = r3.xyzw + -r1.xyzw;
  r3.xyzw = r2.yyyy * r3.xyzw + r1.xyzw;
  r1.xyzw = asint(cb0[7].w) == 1 ? r3.xyzw : r1.xyzw;
  if (asint(cb0[9].y) == 1) {
    r2.xyz = r1.xyz * cb0[9].zzz + cb0[9].www;
    r2.xyzw = t3.Sample(s3_s, r2.xyz).xyzw;
    r2.xyz = r2.xyz + -r1.xyz;
    r1.xyz = cb0[10].xxx * r2.xyz + r1.xyz;
  }
  if (asint(cb0[10].y) == 1) {
    r2.x = dot(r1.xyz, float3(0.219999999,0.707000017,0.0710000023));
    r3.xyzw = t4.Sample(s4_s, r2.xx).xyzw;
    r3.xyzw = r3.xyzw + -r1.xyzw;
    r1.xyzw = cb0[10].zzzz * r3.xyzw + r1.xyzw;
  }
  if (asint(cb0[10].w) == 1) {
    o0.xyzw = v2.x > 0.5 ? r1.xyzw : r0.xyzw;
    return;
  }
  o0.xyzw = r1.xyzw;
  return;
}